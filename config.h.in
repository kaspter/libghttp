#ifndef __CONFIG_H__
#define __CONFIG_H__

/* Version */
#cmakedefine RELEASE_VERSION "@RELEASE_VERSION@"
#cmakedefine BUILD_VERSION   "@BUILD_VERSION@"


/* Project Directory */
#cmakedefine PROJECT_SOURCE_DIR "@PROJECT_SOURCE_DIR@"
#cmakedefine PROJECT_BINARY_DIR "@PROJECT_BINARY_DIR@"

/* Compile Options */
#cmakedefine USE_SSL
#cmakedefine WITH_MBEDTLS
#cmakedefine WITH_WOLFSSL
#cmakedefine WITH_OPENSSL

/* Operate System */
#cmakedefine WIN32
#cmakedefine APPLE
#cmakedefine UNIX

#endif /* __CONFIG_H__ */
